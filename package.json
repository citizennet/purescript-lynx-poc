{
  "name": "purescript-lynx",
  "version": "0.0.1",
  "repository": "git@github.com:thomashoneyman/purescript-lynx",
  "author": "Thomas R. Honeyman <admin@thomashoneyman.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "psc-package install",
    "clean": "rm -rf bower_components node_modules output",
    "build": "pulp build",
    "watch": "pulp --then 'yarn serve' -w build",
    "build-app": "pulp build -I app --to dist/example.js",
    "watch-app": "pulp --then 'yarn serve' -w build -I app --to dist/example.js",
    "serve": "node_modules/json-server/bin/index.js db.json"
  },
  "devDependencies": {
    "pulp": "^12.0.1",
    "purescript": "^0.11.7"
  },
  "dependencies": {
    "json-server": "^0.12.1"
  }
}
