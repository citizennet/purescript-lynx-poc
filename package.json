{
  "name": "purescript-lynx",
  "version": "0.0.1",
  "repository": "git@github.com:thomashoneyman/purescript-lynx",
  "author": "Thomas R. Honeyman <admin@thomashoneyman.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf bower_components node_modules output && rm *.log *.lock",
    "postinstall": "bower i --silent",
    "start": "yarn build && npm-run-all --parallel bundle serve",
    "start-fb": "yarn build-fb && npm-run-all --parallel bundle-fb serve",
    "build": "pulp build -I app",
    "build-fb": "pulp build -I form_builder",
    "watch-fb": "pulp -w build -I form_builder",
    "bundle": "parcel app/index.html",
    "bundle-fb": "parcel form_builder/index.html",
    "serve": "json-server --watch db.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.6",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.7.0",
    "pulp": "^12.0.1",
    "purescript": "^0.11.6"
  },
  "dependencies": {
    "add": "^2.0.6",
    "big-integer": "^1.6.28",
    "json-server": "^0.12.1",
    "yarn": "^1.6.0"
  }
}
